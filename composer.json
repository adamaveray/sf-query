{
  "authors": [
    {
      "name": "Adam Averay",
      "homepage": "https://adamaveray.com.au/"
    }
  ],
  "require": {
    "php": ">=8.2",
    "symfony/http-client": "^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "roave/security-advisories": "dev-latest",
    "symfony/var-dumper": "^7.1",
    "symplify/monorepo-builder": "^11.2"
  },
  "replace": {
    "averay/sfconnect": "self.version"
  },
  "repositories": [
    {
      "type": "path",
      "url": "packages/sfconnect"
    }
  ],
  "autoload": {
    "psr-4": {
      "Averay\\SfConnect\\": "packages/sfconnect/src/"
    },
    "files": [
      "packages/sfconnect/src/lib.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Averay\\SfConnect\\Tests\\": "packages/sfconnect/tests/"
    }
  }
}
