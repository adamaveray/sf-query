{
  "name": "averay/sfconnect",
  "description": "A robust connection interface to the Salesforce REST API.",
  "version": "0.0.2",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Adam Averay",
      "homepage": "https://adamaveray.com.au/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Averay\\SfConnect\\": "src/"
    },
    "files": [
      "src/lib.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Averay\\SfConnect\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.2",
    "symfony/http-client": "^6.0|^7.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^10.5",
    "symfony/var-dumper": "^7.1"
  }
}
